{
  "manifest_version": 3,
  "name": "Random Quote New Tab",
  "version": "1.0",
  "description": "Displays a random quote in a new tab.",
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "chrome_url_overrides": {
    "newtab": "popup.html"
  },
  "icons": {
    "16": "quran.png",
    "48": "quran.png",
    "128": "quran.png"
  }
}